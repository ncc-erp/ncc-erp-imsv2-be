{
  "name": "imsv2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "cross-env TZ=UTC nest start --watch",
    "start:debug": "cross-env TZ=UTC nest start --debug --watch",
    "start:dev": "cross-env ENV=dev node dist/main",
    "start:stg": "cross-env ENV=stg node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env ENV=test jest --detectOpenHandles --config test/jest-e2e.json --testNamePattern=$1",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env ENV=test jest --config test/jest-e2e.json",
    "typeorm": "yarn build && npx typeorm -d dist/config/data-source.config.js",
    "migration:generate": "sh scripts/migration.sh gen local",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:generate:dev": "sh scripts/migration.sh gen dev",
    "migration:run:dev": "cross-env ENV=dev yarn typeorm migration:run",
    "migration:revert:dev": "cross-env ENV=dev yarn typeorm migration:revert",
    "migration:generate:stg": "sh scripts/migration.sh gen stg",
    "migration:run:stg": "cross-env ENV=stg yarn typeorm migration:run",
    "migration:revert:stg": "cross-env ENV=stg yarn typeorm migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/lib-storage": "^3.374.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/passport-jwt": "^3.0.8",
    "axios": "^1.3.2",
    "cache-manager": "^5.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "^122.0.0",
    "moment": "^2.29.4",
    "mssql": "^7.3.5",
    "nest-winston": "^1.8.0",
    "nestjs-cls": "^3.2.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.370.0",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^10.0.5",
    "@types/cron": "^2.0.1",
    "@types/gapi": "^0.0.44",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "jest-html-reporters": "^3.1.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "testcontainers": "^9.9.1",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
